<?xml version="1.0" encoding="utf-8" ?>
<resources>
  <style name="MyTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimary">#2574a9</item>
    <item name="drawerArrowStyle">@style/MyDrawerArrowStyle</item>
  </style>

  <style name="MyDrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
    <item name="color">#f5f5f5</item>
    <item name="spinBars">true</item>
  </style>

  <style name="MyTheme.Splash" parent ="Theme.AppCompat.Light">
    <item name="android:windowBackground">@drawable/About_us_BG1800</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowActionBar">false</item>
  </style>
</resources>
